{
    ds_list_add(lines,"-&gt;" + keyboard_string)
    
    keyboard_string = string_lower(keyboard_string)
    var args = ds_list_create()
    do
    {
        while(string_length(keyboard_string) &gt; 0 &amp;&amp; string_char_at(keyboard_string, 0) = " ") 
            string_delete(keyboard_string,1,1)
        var pos = string_pos(" ", keyboard_string)
        if pos
        {
            ds_list_add(args,string_copy(keyboard_string, 1, pos))
            keyboard_string = string_delete(keyboard_string, 1, pos)
        }
    }
    until(!pos)
    ds_list_add(args,keyboard_string)
    keyboard_string = ""
    
    switch(args[|0])
    {
    case "godmode":
        with(player)
            godmode = !godmode
        break;
    case "help":
        ds_list_add(lines,"Format: command var",
        "Commands: help godmode")
        break;
    default:
        ds_list_add(lines,"Unknown Command","'" + args[|0] +"'")
    }
    if ds_list_size(lines) &gt; lines_saved
        ds_list_delete(lines,0)
}
var str = keyboard_string
io_clear();
keyboard_string = str
    
</string>
