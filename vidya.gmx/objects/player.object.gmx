        <arguments>
          <argument>
            <kind>1</kind>
            <string>//D E M O 
if keyboard_check(ord('P')) &amp;&amp; keyboard_check(ord('L')) &amp;&amp; keyboard_check(ord('S')) {room_goto(BOSS_TWO); x = 512; y = 1472}
if keyboard_check(ord('A')) &amp;&amp; keyboard_check(ord('G')) &amp;&amp; keyboard_check(ord('D')) {room_goto(BOSS_ONE); x = 704 ; y = 768}
if keyboard_check_pressed(ord('O')) {display *= -1}
if keyboard_check_pressed(ord('P')) {comfymode *= -1}

//equipment code
if weapon != pweapon {instance_destroy(pweapon); pweapon = weapon; instance_create(x, y, weapon)}
if guard != pguard {instance_destroy(pguard); pguard = guard; instance_create(x, y, guard)}

if keyboard_check_released(ord('E')) 
{
if ds_list_find_index(weaponstock, weapon) + 2 &gt; ds_list_size(weaponstock) {weapon = ds_list_find_value(weaponstock, 0)} else
weapon = ds_list_find_value(weaponstock, ds_list_find_index(weaponstock, weapon) + 1)
}

if keyboard_check_released(ord('Q')) 
{
if ds_list_find_index(guardstock, guard) + 2 &gt; ds_list_size(guardstock) {guard = ds_list_find_value(guardstock, 0)} else
guard = ds_list_find_value(guardstock, ds_list_find_index(guardstock, guard) + 1)
}


//death code

if hp &lt;= 0 &amp;&amp; alive = true {pothp = 0; mana = 0;/* ini_open('savedata.ini'); ini_read_real(data, deaths, +1); ini_close();*/ instance_create(x, y, YOUDIED); alive = false}
if alive = false {image_alpha = 0}
with weapon {image_alpha = player.image_alpha}
with guard {image_alpha = player.image_alpha}

//hp code

if prevhp &gt; hp &amp;&amp; inv = false {prevhp -= 1}
if prevhp &lt; hp {prevhp = hp}
if hp &gt; maxhp {mana += hp mod maxhp / 2; hp = maxhp}
if pothp &gt; 0 {pothp -=1; hp += healstat; mana += healstat / 4}
if (mana &gt; 50) {mana = 50}

if keyboard_check_pressed(ord('R'))
{
if mana != 50 {error = "Insufficient Blood"; alarm[8] = 50} else
if distance_to_object(spawnpoint) &lt; 500 {error = "Ritual Interference"; alarm[8] = 50} else
if interference = true {error = "Magical Interference"; alarm[8] = 50} else
{instance_create(x, y, spawnpoint); mana = 0}
}

//death code
if alarm[8] &gt; 0
{}

//movement code
if alive = true

{

if keyboard_check(ord('W')) &amp;&amp; place_free(x,y-spd) { 
    if keyboard_check(ord('A')) &amp;&amp; place_free(x-spd,y){
        dir = 135;
    } else if keyboard_check(ord('D')) &amp;&amp; place_free(x+spd,y) {
        dir = 45;
    } else { 
        dir = 90;
    }
} else if keyboard_check(ord('S')) &amp;&amp; place_free(x,y+spd) {
    if keyboard_check(ord('A')) &amp;&amp; place_free(x-spd,y) {
        dir = 225;
    } else if keyboard_check(ord('D')) &amp;&amp; place_free(x+spd,y) {
        dir = 315;
    } else {
        dir = 270;
    }
} else {
    if keyboard_check(ord('A')) &amp;&amp; place_free(x-spd,y) {
        dir = 180;
    } else if keyboard_check(ord('D')) &amp;&amp; place_free(x+spd,y){
        dir = 0;
    } else {
        dir = -1;
    } 
}

if dir != -1 controller_active = false
var gp_h = gamepad_axis_value(0,gp_axislh)
var gp_v = gamepad_axis_value(0,gp_axislv)
gamepad_set_axis_deadzone(0,.2)
if gp_h != 0 or gp_v != 0
{
    dir = point_direction(0,0,gp_h,gp_v)
    if !place_free(x+lengthdir_x(spd,dir),y+lengthdir_y(spd,dir))
        dir = -1
    controller_active = true
}


if (dir != -1) 

{
if keyboard_check(vk_shift) &amp;&amp; exhausted = false {if spd &lt; 6 {spd += 0.5}} else {if spd &lt; 3.5 {spd += 0.5}; if spd &gt; 3.5 {spd -= 0.5}}
prevdir = dir
}

else

{
if spd &gt; 0 {spd -= 0.5}
}

if spd &lt; 0 {spd = 0}

if place_free(x+lengthdir_x(spd+1,prevdir),y+lengthdir_y(spd+1,prevdir))
{
x += lengthdir_x(spd, prevdir);
y += lengthdir_y(spd, prevdir);
}

}

//stamina code

if (x != xprevious or y != yprevious) and (keyboard_check(vk_shift) or gamepad_button_check(0,gp_shoulderl)) &amp;&amp; exhausted = false {stamina -= 0.2; sprinting = true} else {if stamina &lt; maxstamina {stamina+=0.5}; sprinting = false}
if stamina &lt;= 0 {exhausted = true}
if exhausted = true {stamina-= 0.4}
if exhausted = true &amp;&amp; stamina &gt;= maxstamina {exhausted = false}
if stamina &gt; maxstamina {stamina = maxstamina}
if mouse_xprevious != device_mouse_x_to_gui(0) or mouse_yprevious != device_mouse_y_to_gui(0) or controller_active = false
{
    image_angle = point_direction(x, y, mouse_x, mouse_y)
    controller_active = false
}
mouse_xprevious = device_mouse_x_to_gui(0)
mouse_yprevious = device_mouse_y_to_gui(0)
var gp_h = gamepad_axis_value(0,gp_axisrh)
var gp_v = gamepad_axis_value(0,gp_axisrv)
if gp_h != 0 or gp_v != 0
{
    image_angle = point_direction(0,0,gp_h,gp_v)
    controller_active = true
}
if inv = 1 {image_index = 1} else image_index = 0

// fullscreen
if view_wview != window_get_width() or view_hview != window_get_height()
{
surface_free(global.darkness)
view_wview = window_get_width()
view_hview = window_get_height()
view_wport = view_wview
view_hport = view_hview
surface_resize(application_surface, view_wport, view_hport); 
}
</string>
