            <string>///load an image

if bgc &lt;= bge
{
if sc &lt;= se
{
    var w = sprite_get_width(sc)
    var h = sprite_get_height(sc)
}
else
{
    var w = background_get_width(bgc)
    var h = sprite_get_height(bgc)
}
var surf = surface_create(w,h)
surface_set_target(surf)
if sc &lt;= se
{
    var p = imp[|irandom(ds_list_size(imp))]
    while(is_undefined(p)){p = imp[|irandom(ds_list_size(imp))]}
    var ind = sprite_add(working_directory + "JUCIEY/" + p,0,0,0,sprite_get_xoffset(sc),sprite_get_yoffset(sc))
    show_debug_message(string(ind) + " " + imp[|irandom(ds_list_size(imp) - 1)])
    draw_sprite_stretched(ind,0,0,0,w,h)
    draw_text(0,0," ") //it will be white without this
    sprite_delete(ind)
    surface_reset_target()
    ds_list_add(im[|sc],sprite_create_from_surface(surf, 0, 0, w, h, false, false, sprite_get_xoffset(sc), sprite_get_yoffset(sc)))
    var n = ds_list_size(im[|sc])
    if n &gt;= sprite_get_number(sc)
    {
        ++sc
        show_debug_message("loaded " + string(sc + bgc) + " / " + string(se + bge))
    }
    else
        show_debug_message("loaded " + string(sc + bgc) + "." + string(n) + " / " + string(se + bge))
}
else
{
    var tw = w div 64
    var th = h div 64
    for(var twc = 0; twc &lt; tw; ++twc)
    for(var thc = 0; thc &lt; th; ++thc)
    {
        var p = imp[|irandom(ds_list_size(imp))]
        while(is_undefined(p)){p = imp[|irandom(ds_list_size(imp))]}
        var ind = sprite_add(working_directory + "JUCIEY/" + p,0,0,0,0,0)
        draw_sprite_stretched(ind,0,twc * 64,thc * 64,64,64) 
    }
    draw_text(0,0," ")
    ds_list_add(im,background_create_from_surface(surf, 0, 0, w, h, false, false))
    ++bgc
    show_debug_message("loaded " + string(sc + bgc) + " / " + string(se + bge))
}

surface_free(surf)
}
else if snc &lt; ds_list_size(sn)
{
    sn[|snc] = audio_create_stream(working_directory + "THICC/" + sn[|snc]);
    ++snc
    show_debug_message("loaded " + string(snc) + " / " + string(ds_list_size(sn)))
}
else
{
    show_debug_message("DEPLOYING")
    for(sc = 0; sc &lt;= se; ++sc)
    {
        show_debug_message(string(sc + bgc) + " / " + string(se + bge))
        sprite_assign(sc, ds_list_find_value(im[|sc],0));
        for(var i = 1; i &lt; ds_list_size(im[|sc]); ++i)
        {
            sprite_merge(sc, ds_list_find_value(im[|sc],i));
        }
    }
    for(bgc = 0; bgc &lt;= bge; ++bgc)
    {
        show_debug_message(string(sc + bgc) + " / " + string(se + bge))
        background_assign(bgc,im[|sc + bgc])
    }
    
    for(snc = 0; snc &lt;= sne; ++snc)
    {
        if ds_list_size(sn) &gt; 0
            ds_list_add(snm,sn[|irandom(ds_list_size(sn)-1)])
        else
            ds_list_add(snm,moo)
    }
    var f = font_add("f.ttf",12,0,0,32,128)
    draw_set_font(f)
    audio_play_sound(moo,0,1)
    m = 99
}
if m != 99
    alarm[0] = 1

</string>
